<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewData["Title"] - MacroFit</title>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter&amp;display=swap">
    <link rel="stylesheet" href="~/css/styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="dnav dnav-desktop">
            <div class="dnav-side"><a class="dynamic-btn" asp-page="/Index"><img class="dnav-image dnav-image-brand" src="~/images/brand.png"></a></div>
            <div class="dnav-centre">
                @{
                    string pageName = ViewContext.RouteData.Values["page"]?.ToString();
                    string folderName = "";
                    if (!string.IsNullOrEmpty(pageName))
                    {
                        string[] segments = pageName.Split('/');
                        if (segments.Length > 2)
                        {
                            folderName = segments[1];
                        }
                    }
                    Func<string, string> isSelected = (page) => folderName.Equals(page, StringComparison.OrdinalIgnoreCase) ? "selected" : "";
                }
                <a class="dnav-button dynamic-btn" asp-page="/Foods/Index">
                    <div class="dnav-button-wrapper @isSelected("Foods")">
                        <span class="dnav-button-icon">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 -32 576 576" width="1em" height="1em" fill="currentColor">
                                <!--! Font Awesome Free 6.1.1 by fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free (Icons: CC BY 4.0, Fonts: SIL OFL 1.1, Code: MIT License) Copyright 2022 Fonticons, Inc. -->
